{
    "extends": [
      "airbnb-base",
      "plugin:jest/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "script"
    },
    "plugins": [
      "import",
      "jest"
    ],
    "env": {
        "node": true,
        "jest/globals": true
    },
    "rules":{
        "no-console": 0
    }
}
